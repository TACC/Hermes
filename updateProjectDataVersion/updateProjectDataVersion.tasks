function taskMain()
   task{"ReadProject"}

   task{"CmdLineOptions"}

   local masterTbl = masterTbl()
   if (masterTbl.version) then
      task{"GetVersion"}
   else
      task{"WriteProjectData"}
   end

   return 0
end


